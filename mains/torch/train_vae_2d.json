{
		"name": "DeepSMLM",
		"n_gpu": 1,

		"arch": {
				"type": "LocalizationVAE",
				"args": {"latent_dim": 10, "nx": 20, "ny": 20}
		},
		"data_loader": {
		         "path":"/home/cwseitz/Desktop/Torch/Train2D/"},
		"optimizer": {
				"type": "Adam",
				"args":{
						"lr": 0.005,
						"weight_decay": 0,
						"amsgrad": true
				}
		},
		"loss": "vae_loss",
		"lr_scheduler": {
				"type": "StepLR",
				"args": {
						"step_size": 1,
						"gamma": 1
				}
		},
		"trainer": {
				"epochs": 50,
				"save_dir": "/home/cwseitz/Desktop/Torch/Models2D/",
				"save_period": 10,
				"verbosity": 2,
				"monitor": "min val_loss",
				"early_stop": 10,
				"tensorboard": true
		},
		"metrics": []
}
