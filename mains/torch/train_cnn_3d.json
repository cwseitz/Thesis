{
		"name": "SMLM",
		"n_gpu": 0,

		"arch": {
				"type": "LocalizationCNN",
				"args": {"nz": 40, "scaling_factor": 800, "dilation_flag": true}
		},
		"data_loader": {
		         "path":"/home/cwseitz/Desktop/Torch/Train3D_800nm/"},
		"optimizer": {
				"type": "Adam",
				"args":{
						"lr": 0.005,
						"weight_decay": 0,
						"amsgrad": true
				}
		},
		"loss": "KDE_loss3D",
		"lr_scheduler": {
				"type": "StepLR",
				"args": {
						"step_size": 1,
						"gamma": 1
				}
		},
		"trainer": {
				"epochs": 50,
				"save_dir": "saved/",
				"save_period": 10,
				"verbosity": 2,
				"monitor": "min val_loss",
				"early_stop": 10,
				"tensorboard": true
		},
		"metrics": []
}
